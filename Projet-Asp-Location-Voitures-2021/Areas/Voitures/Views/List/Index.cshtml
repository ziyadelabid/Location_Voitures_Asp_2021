    <div class="car_section sec_ptb_100 clearfix">
        <div class="container">
            <div class="row justify-content-lg-between justify-content-md-center justify-content-sm-center">

                <div class="col-lg-4 col-md-6 col-sm-8 col-xs-12" id="filterSidePanel">
                    <aside class="filter_sidebar sidebar_section" data-bg-color="#F2F2F2">
                        <div class="sidebar_header" data-bg-gradient="linear-gradient(90deg, #0C0C0F, #292D45)">
                            <h3 class="text-white mb-0">Filtres</h3>
                        </div>
                        <div class="sb_widget">

                            @using (Ajax.BeginForm("FilteredVoitures", "List", null, ajaxOptions, new { id = "filterForm" }))
                            {
                                @Html.AntiForgeryToken()
                                <div class="sb_widget" data-aos="fade-up" data-aos-delay="100">
                                    <div class="form_item">
                                        <h4 class="input_title">Prix</h4>

                                        @Html.DropDownListFor((model) => model.FirstOrDefault().Prix, new SelectList(ViewBag.prixItems), "Selectionnez prix", htmlAttributes: new { @class = "formTest" })
                                    </div>

                                    <div class="form_item">
                                        <h4 class="input_title">Marque</h4>

                                        @Html.DropDownListFor((model) => model.FirstOrDefault().Marque, new SelectList(ViewBag.marqueItems), "Selectionnez marque", htmlAttributes: new { @class = "formTest" })

                                    </div>

                                    <div class="form_item">
                                        <h4 class="input_title">Carburant</h4>

                                        @Html.DropDownListFor((model) => model.FirstOrDefault().Carburant, new SelectList(ViewBag.carburantItems), "Selectionnez carburant", htmlAttributes: new { @class = "formTest" })
                                    </div>

                                    <div class="form_item">
                                        <h4 class="input_title">Boite a vitesse</h4>

                                        @Html.DropDownListFor((model) => model.FirstOrDefault().Boite_Vitesse, new SelectList(ViewBag.boiteVitesseItems), "Selectionnez Boite a vitesse", htmlAttributes: new { @class = "formTest" })
                                    </div>
                                    <div class="form_item">
                                        <h4 class="input_title">Emplacement de prise</h4>
                                        <div class="position-relative">
                                            <input id="location_two" type="text" name="location" placeholder="Ville">
                                            <label for="location_two" class="input_icon"><i class="fas fa-map-marker-alt"></i></label>
                                        </div>
                                    </div>
                                    <div class="form_item">
                                        <h4 class="input_title">Date de reservation</h4>
                                        <input type="date" name="date">
                                    </div>
                                </div>


                                <div class="sb_widget" data-aos="fade-up" data-aos-delay="100">
                                    <h4 class="input_title">Nombre de passagers:</h4>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="checkbox_input">
                                                <label for="passengers_radio1"><input type="radio" value="2" id="passengers_radio1" name="passagers" class="toggle_checkbox"> 2</label>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="checkbox_input">
                                                <label for="passengers_radio2"><input type="radio" value="5" id="passengers_radio2" name="passagers" class="toggle_checkbox"> 5</label>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="checkbox_input">
                                                <label for="passengers_radio3"><input type="radio" value="4" id="passengers_radio3" name="passagers" class="toggle_checkbox"> 4</label>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="checkbox_input">
                                                <label for="passengers_radio4"><input type="radio" value="plus" id="passengers_radio4" name="passagers" class="toggle_checkbox"> Plus</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="sb_widget" data-aos="fade-up" data-aos-delay="100">
                                    <h4 class="input_title">Nombre de portes:</h4>
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="checkbox_input">
                                                <label for="portes_radio1"><input type="radio" value="2" id="portes_radio1" name="portes"> 2</label>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="checkbox_input">
                                                <label for="portes_radio2"><input type="radio" value="4" id="portes_radio2" name="portes"> 4</label>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <hr data-aos="fade-up" data-aos-delay="100">

                                <div data-aos="fade-up" data-aos-delay="100">
                                    <button type="submit" id="filterFormSubmitBtn" class="custom_btn bg_default_red text-uppercase">Valider <img src="~/assets/images/icons/icon_01.png" alt="icon_not_found"></button>
                                </div>
                            }
                        </div>
                    </aside>
                </div>
                <!--   After FILTER Button clicked SIDE PANEL     -->
                <div class="col-lg-4 col-md-6 col-sm-8 col-xs-12" id="filterSidePanel2" style="display:none">
                    <aside class="filter_sidebar sidebar_section" data-bg-color="#F2F2F2">
                        <div class="sidebar_header" data-bg-gradient="linear-gradient(90deg, #0C0C0F, #292D45)">
                            <h3 class="text-white mb-0">Details</h3>
                        </div>
                        <div class="sb_widget">
                            <h6 class="item_available" id="filterSidePanel2_h6"></h6>
                            <hr data-aos="fade-up" data-aos-delay="100">
                            <button type="button" id="retourBtn" class="custom_btn bg_default_blue text-uppercase">Retour </button>
                        </div>
                    </aside>
                </div>
                <!--   ----------------------------------------     -->
                <div id="voituresListParentContainer" class="col-lg-8 col-md-10 col-sm-12 col-xs-12">
                    <div id="VoituresContainer">
                        <div class="item_shorting clearfix" data-aos="fade-up" data-aos-delay="100">
                            <div class="row align-items-center justify-content-lg-between">
                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                    <span class="item_available">Offres disponibles @Html.Raw(Model.TotalItemCount)  </span>
                                </div>

                                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                    @using (Html.BeginForm("Index", "List", FormMethod.Get, new { id = "sort_selectform" }))
                                    {
                                        <div class="form_item mb-0">
                                            <select name="DisponibleFilter" class="sort_select">
                                                <option value="ALL" data-display="Toutes les voitures" @Html.Raw(ViewBag.SelectedValue == "ALL" ? "selected" : "")>Toutes les voitures</option>
                                                <option value="DISPONIBLES" data-display="Voitures disponibles" @Html.Raw(ViewBag.SelectedValue == "DISPONIBLES" ? "selected" : "")>Voitures disponibles</option>
                                            </select>
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                        <br />
                        <br />
                        <div class="row">
                            @if (Model.Count > 0)
                            {
                                foreach (var voiture in Model)
                                {
                                    <div class="col-lg-6 col-md-6">
                                        <div class="feature_vehicle_item" data-aos="fade-up" data-aos-delay="100">
                                            <h3 class="item_title mb-0">
                                                <a href="#!">
                                                    @Html.DisplayFor(modelItem => voiture.Marque)
                                                </a>
                                            </h3>
                                            <div class="item_image position-relative">
                                                <a class="image_wrap" href="@Url.Action("Index", "DetailVoit", new { id = voiture.Id })">
                                                    <img src=@Url.Content("~/images/" + voiture.Image_Name) alt="image_not_found">
                                                </a>
                                                @if (voiture.Promotion == null)
                                                {
                                                    <span class="item_price bg_default_blue">
                                                        @Html.DisplayFor(modelItem => voiture.Prix) DH/Jour
                                                    </span>
                                                }
                                                else
                                                {
                                                    <span class="item_price bg_default_blue" style="bottom: 52px!important">
                                                        <s>@Html.DisplayFor(modelItem => voiture.Prix) DH/Jour</s>
                                                    </span>
                                                    <span class="item_price" style="background-color: #b98200">
                                                        @Html.DisplayFor(modelItem => voiture.Promotion) DH/Jour
                                                    </span>
                                                }
                                                @if (voiture.DateRetour != null && voiture.DateRetour >= DateTime.Now && voiture.DateReservation <= DateTime.Now)
                                                {
                                                    <span class="item_price bg_default_red" style="left: 70%!important">Reservé</span>
                                                }
                                            </div>
                                            <ul class="info_list ul_li_center clearfix">
                                                <li>@Html.DisplayFor(modelItem => voiture.Boite_Vitesse)</li>
                                                <li>@Html.DisplayFor(modelItem => voiture.Portes) Portes</li>
                                                <li>@Html.DisplayFor(modelItem => voiture.Places) Passagers</li>
                                                <li>@Html.DisplayFor(modelItem => voiture.Carburant)</li>
                                            </ul>
                                        </div>
                                    </div>
                                }
                            }
                            else
                            {
                                <div class="col-lg-10 col-md-10" data-aos="fade-up" data-aos-delay="100">
                                    <h3>Aucune donnée a afficher</h3>
                                </div>
                            }
                        </div>

                        <div class="pagination_wrap clearfix" data-aos="fade-up" data-aos-delay="100">
