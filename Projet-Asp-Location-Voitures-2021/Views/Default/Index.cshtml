        <!-- cars_section - start
    ================================================== -->
        <br />
        <br />
        <section class="cars_section clearfix">

            <div class="row justify-content-center">
                <div class="col-lg-6 col-md-8 col-sm-12 col-xs-12">
                    <div class="section_title mb_60 text-center" data-aos="fade-up" data-aos-delay="100">
                        <h2 class="title_text mb_15">
                            <span>Voitures en promotion</span>
                        </h2>
                        <p class="mb-0">
                            Posez la souris sur la voiture pour en savoir plus
                        </p>
                    </div>
                </div>
            </div>
            @if (Model.promotionVoitures.Count() > 0)
            {
                <div class="offers_car_carousel slideshow4_slider" data-slick='{"dots": false}' data-aos="fade-up" data-aos-delay="100">
                    @foreach (var voiture in Model.promotionVoitures)
                    {
                        <div class="item">
                            <div class="gallery_fullimage_2">
                                <img src=@Url.Content("~/images/" + voiture.Image_Name) alt="image_not_found">
                                <div class="item_content text-white">
                                    <s class="item_price bg_default_blue">@Html.Raw(voiture.Prix) DH/Jour</s>
                                    <span class="item_price" style="background-color:#b98200;">@Html.Raw(voiture.Promotion) DH/Jour</span>
                                    <h3 class="item_title text-white">@Html.Raw(voiture.Marque)</h3>
                                    <a class="text_btn text-uppercase" href="@Url.Action("Index", "Voitures/Details", new { id = voiture.Id })"><span>Reserver la voiture</span> <img src="~/assets/images/icons/icon_02.png" alt="icon_not_found"></a>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            }
            else
            {
                <h4 style="text-align:center" data-aos="fade-up" data-aos-delay="100">Aucune donnée a afficher</h4>
            }
        </section>
            <br />
            <br />
            <br />
        <hr style="width: 60%;margin:auto;" />
        <!-- cars_section - end
    ================================================== -->
        <!-- feature_section - start
    ================================================== -->
        <section class="feature_section sec_ptb_150 clearfix" data-bg-color="#F2F2F2">
            <div class="container">

                <div class="row justify-content-center">
                    <div class="col-lg-6 col-md-8 col-sm-12 col-xs-12">
                        <div class="section_title mb_60 text-center" data-aos="fade-up" data-aos-delay="100">
                            <h2 class="title_text mb_15">
                                <span>Voitures les plus reservées</span>
                            </h2>
                            <p class="mb-0">
                                Liste des voitures les plus demandées
                            </p>
                        </div>
                    </div>
                </div>

                @if (Model.featuredVoitures.Count() > 0)
                {
                    <div class="feature_vehicle_filter element-grid clearfix">

                        @foreach (var voiture in Model.featuredVoitures)
                        {
                            <div class="element-item ">
                                <div class="feature_vehicle_item" data-aos="fade-up" data-aos-delay="100">
                                    <h3 class="item_title mb-0">
                                        <a href="#!">
                                            @Html.Raw(voiture.Marque)
                                        </a>
                                    </h3>
                                    <div class="item_image position-relative">
                                        <a class="image_wrap" href="@Url.Action("Index", "Voitures/DetailVoit", new { id = voiture.Id })">
                                            <img src=@Url.Content("~/images/" + voiture.Image_Name) alt="image_not_found">
                                        </a>
                                        @if (voiture.Promotion == null)
                                        {
                                            <span class="item_price bg_default_blue">
                                                @Html.DisplayFor(modelItem => voiture.Prix) DH/Jour
                                            </span>
                                        }
                                        else
                                        {
                                            <span class="item_price bg_default_blue" style="bottom: 52px!important">
                                                <s>@Html.DisplayFor(modelItem => voiture.Prix) DH/Jour</s>
                                            </span>
                                            <span class="item_price" style="background-color: #b98200">
                                                @Html.DisplayFor(modelItem => voiture.Promotion) DH/Jour
                                            </span>
                                        }
                                    </div>
                                    <ul class="info_list ul_li_center clearfix">
                                        <li>@Html.DisplayFor(modelItem => voiture.Boite_Vitesse)</li>
                                        <li>@Html.DisplayFor(modelItem => voiture.Portes) Portes</li>
                                        <li>@Html.DisplayFor(modelItem => voiture.Places) Passagers</li>
                                        <li>@Html.DisplayFor(modelItem => voiture.Carburant)</li>
                                    </ul>
                                </div>
                            </div>
                        }
                    </div>
                }
                else
                {
                    <h4 style="text-align:center" data-aos="fade-up" data-aos-delay="100">Aucune donnée a afficher</h4>
                    <br />
                }
                <div class="abtn_wrap text-center clearfix" data-aos="fade-up" data-aos-delay="100">
                    <a class="custom_btn bg_default_red btn_width text-uppercase" href="@Url.Action("Index", "Voitures/List")">Reserver une voiture <img src="~/assets/images/icons/icon_01.png" alt="icon_not_found"></a>
                </div>

            </div>
        </section>
