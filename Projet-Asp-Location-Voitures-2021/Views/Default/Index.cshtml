        <section class="slider_section text-white text-center position-relative clearfix">
            <div class="main_slider clearfix">
                <div class="item has_overlay d-flex align-items-center" data-bg-image="/assets/images/backgrounds/bg_photo4.jpg">
                    <div class="overlay"></div>
                    <div class="container">

                        <div class="row justify-content-center">
                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                                <div class="slider_content text-center">
                                    <h3 class="text-white text-uppercase" data-animation="fadeInUp" data-delay=".3s">Commodité</h3>
                                    <p data-animation="fadeInUp" data-delay=".5s">
                                        Le plus haut niveau de comfort
                                    </p>
                                    <div class="abtn_wrap clearfix" data-animation="fadeInUp" data-delay=".7s">
                                        <a class="custom_btn bg_default_red btn_width text-uppercase" href="@Url.Action("Index","Voitures/List")">Reserver maintenant <img src="~/assets/images/icons/icon_01.png" alt="icon_not_found"></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="item has_overlay d-flex align-items-center" data-bg-image="/assets/images/backgrounds/bg_photo3.jpg">
                    <div class="overlay"></div>
                    <div class="container">

                        <div class="row justify-content-center">
                            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
